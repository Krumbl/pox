{
  "name": "pox",
  "version": "0.0.0",
  "main": "dist/electron.js",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --names 'CLIENT,SERVER' --prefix-colors 'yellow,blue' --kill-others \"npm run start:angular\" \"npm run start:electron\"",
    "build": "npm run build:angular && npm run build:electron",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:angular": "ng build",
    "start:angular": "ng serve",
    "build:electron": "ng build --prod --base-href ./ && tsc --lib ES2018,DOM --target ES5 electron/electron.ts --outDir dist",
    "start:electron": "electron .",
    "package": "npm run build && electron-packager . pox --overwrite --asar --ignore=^e2e$ --ignore=^src$ --ignore=^electron$ --ignore=^.editorconfig$ --ignore=^.gitignore$ --ignore=^angular.json$ --ignore=^browserslist$ --ignore=^karma.conf.js$ --ignore=^package-lock.json$ --ignore=^README.md$ --ignore=^tslint --ignore=^tsconfig"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.9",
    "@angular/cdk": "^10.2.5",
    "@angular/common": "~10.0.9",
    "@angular/compiler": "~10.0.9",
    "@angular/core": "~10.0.9",
    "@angular/forms": "~10.0.9",
    "@angular/material": "^10.2.5",
    "@angular/platform-browser": "~10.0.9",
    "@angular/platform-browser-dynamic": "~10.0.9",
    "@angular/router": "~10.0.9",
    "is-electron": "^2.2.0",
    "pretty-bytes": "^5.4.1",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.6",
    "@angular/cli": "~10.0.6",
    "@angular/compiler-cli": "~10.0.9",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.3.0",
    "electron": "^10.1.3",
    "electron-packager": "^15.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
}
